@article{Shepsle:2012vl,
	Address = {Stanford U},
	An = {1023523105; 1303136},
	Annote = {Gordon Tullock, nearly a half century ago, raised questions about Arrow's Theorem ("a phantom has stalked the classrooms and seminars of economics and political science"). He followed this up by asking, in light of Arrow's Theorem, "Why so much stability?" In this paper, a more nuanced understanding of the operating characteristics of majority rule in institutional settings, anticipated and stimulated by Tullock, is spelled out. A major distinction is made between preference cycles and voting cycles, suggesting why Arrow's phantom still stalks, but that Tullock's intuitions are germane as well.},
