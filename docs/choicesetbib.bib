@article{Dekel:2009sv,
	Address = {U Cambridge and U MN},
	An = {56936168; 1054779},
	Annote = {What behaviour can be explained using the hypothesis that the agent faces temptation but is otherwise a "standard rational agent"? In earlier work, Gul and Pesendorfer (2001) use a set betweenness axiom to restrict the set of preferences considered by Dekel, Lipman and Rustichini (2001) to those explainable via temptation. We argue that set betweenness rules out plausible and interesting forms of temptation including some which may be important in applications. We propose a pair of alternative axioms called DFC, desire for commitment, and AIC, approximate improvements are chosen. DFC characterizes temptation as situations in which given any set of alternatives, the agent prefers committing herself to some particular item from the set rather than leaving herself the flexibility of choosing later. AIC is based on the idea that if adding an option to a menu improves the menu, it is because that option is chosen under some circumstances. From this interpretation, the axiom concludes that if an improvement is worse (as a commitment) than some commitment from the menu, then the best commitment from the improved menu is strictly preferred to facing that menu. We show that these axioms characterize a natural generalization of the Gul-Pesendorfer representation.},
	Annote1 = {Date revised - 2009-08-01},
	Annote2 = {Language of summary - English},
	Annote3 = {Pages - 937-971},
	Annote4 = {ProQuest ID - 56936168},
	Annote5 = {Availability - {$[$}URL:http://www.blackwellpublishing.com/journal.asp?ref=0034-6527{$]$} Publisher's URL},
	Annote6 = {Last updated - 2010-09-24},
	Annote7 = {British nursing index edition - Review of Economic Studies, vol. 76, no. 3, July 2009, pp. 937-71},
	Annote8 = {Corporate institution author - Dekel, Eddie; Lipman, Barton L; Rustichini, Aldo},
	Annote9 = {DOI - econlit-1054779; 1054779; 0034-6527},
