@article{Myerson:2007ce,
	Address = {U Chicago},
	An = {56642178; 0937576},
	Annote = {The core is extended to games with incomplete information. The feasible set is characterized by incentive-compatible mechanisms. Blocking is organized at the interim stage by an incentive-compatible mediation plan. Membership of the blocking coalition itself may be determined randomly by the blocking mediator. Nonemptiness of an interim fine core is proven for games with a balanced structure, independent types, and sidepayments. An offer of severance payments may be needed to inhibit blocking. Core allocations are characterized in terms of virtual-utility scales that generalize the weighted-utility scales of the inner core. Mechanisms that achieve core allocations are coalitionally durable.},
