@article{Goeree:2007nb,
	Address = {Unlisted},
	An = {56628120; 0930811},
	Annote = {We report experimental results from long sequences of decisions in environments that are theoretically prone to severe information cascades. Observed behaviour is much different--information cascades are ephemeral. We study the implications of a theoretical model based on quantal response equilibrium, in which the observed cascade formation/collapse/formation cycles arise as equilibrium phenomena. Consecutive cascades may reverse states, and usually such a reversal is self-correcting: the cascade switches to the correct state. These implications are supported by the data. We extend the model to allow for base rate neglect and find strong evidence for overweighting of private information. The estimated belief trajectories indicate fast and efficient learning dynamics.},
