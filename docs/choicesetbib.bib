@article{Alesina:2002cp,
	Address = {U Bocconi and IGIER},
	An = {56182004; 0631206},
	Annote = {Both individual experiences and community characteristics influence how much people trust each other. Using individual level data drawn from US localities we find that the strongest factors associated with low trust are: (i) a recent history of traumatic experiences; (ii) belonging to a group that historically felt discriminated against, such as minorities (blacks in particular) and, to a lesser extent, women; (iii) being economically unsuccessful in terms of income and education; (iv) living in a racially mixed community and/or in one with a high degree of income disparity. Religious beliefs and ethnic origins do not significantly affect trust. The role of racial cleavages leading to low trust is confirmed when we explicitly account for individual preferences on inter racial relationships: within the same community, individuals who express stronger feelings against racial integration trust relatively less the more racially heterogeneous the community is.},
	Annote1 = {Date revised - 2003-01-01},
