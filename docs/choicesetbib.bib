@article{Irwin:1996hq,
	Address = {U Chicago},
	An = {56717239; 0412412},
	Annote = {The authors analyze Senate roll-call votes concerning tariffs on specific goods in order to understand the economic and political factors influencing the passage of the Smoot-Hawley Tariff Act of 1930. Contrary to recent studies emphasizing the partisan nature of the Congressional votes, their reading of the debates in the Congressional Record suggests that the final, party-line voting masks a rich vote-trading dynamic. The authors estimate a logit model of specific tariff votes that permits them to identify (1) important influences of specific producer beneficiaries in each Senator's constituency and (2) log-rolling coalitions among Senators with otherwise unrelated constituency interests which succeeded in raising tariff rates.},
